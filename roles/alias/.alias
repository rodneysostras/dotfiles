   alias ..="cd .."
   alias cat="cat"
   alias ls="exa --icons --group-directories-first"
   alias ll="exa --icons --group-directories-first -lah"
   # alias code="code ."
   alias dev="devcontainer open"
   # alias jup="jupyter notebook --no-browser"
   alias port="lsof -i -P -n"
   alias unpm="find . -name 'node_modules' -type d -prune -exec rm -rf '{}' ';'"
   alias inpm="find . -name 'package.json' -type f -not -path '*/node_modules/*' -execdir npm i --silent --no-progress \;"
   alias awslc="aws --endpoint-url=${LOCALSTACK_URL:-http://localhost:4566} --profile=localstack"
